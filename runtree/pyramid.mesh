# ldk mesh file
# Format for complex types
# vertex           pos_x pos_y pos_z normal_x normal_y normal_z uv_x uv_y
# material         id name
# surface          material_id index_start index_count
# bounding_box     min_x min_y min_z max_x max_y max_z
# bounding_sphere  center.x center.y center.z radius

version 1.0
vertex_format PNU
vertex_count 16
index_count 18
material_count 1
surface_count 1

